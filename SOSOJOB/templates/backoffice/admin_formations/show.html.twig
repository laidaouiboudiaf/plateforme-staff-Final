<!DOCTYPE html>

<title>InfosFormations</title>

{% block body %}
    <h1>InfosFormations</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>NomEtablissement</th>
                <td>{{ infos_formation.nomEtablissement }}</td>
            </tr>
            <tr>
                <th>DomaineEtudes</th>
                <td>{{ infos_formation.domaineEtudes }}</td>
            </tr>
            <tr>
                <th>Diplome</th>
                <td>{{ infos_formation.diplome }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ infos_formation.debut ? infos_formation.debut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fin</th>
                <td>{{ infos_formation.fin ? infos_formation.fin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>IdFormations</th>
                <td>{{ infos_formation.idFormations }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_formations_index') }}">back to list</a>

    <a href="{{ path('admin_formations_edit', {'idFormations': infos_formation.idFormations}) }}">edit</a>

    {{ include('backoffice/admin_formations/_delete_form.html.twig') }}
{% endblock %}
