<!DOCTYPE html>

<title>Missions</title>

{% block body %}
    <h1>Missions</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdMissions</th>
                <td>{{ mission.idMissions }}</td>
            </tr>
            <tr>
                <th>ObjetMissions</th>
                <td>{{ mission.objetMissions }}</td>
            </tr>
            <tr>
                <th>DescriptionMissions</th>
                <td>{{ mission.descriptionMissions }}</td>
            </tr>
            <tr>
                <th>CategorieMissions</th>
                <td>{{ mission.typeDemission }}</td>
            </tr>
            <tr>
                <th>DateDebutMissionsPr</th>
                <td>{{ mission.dateDebutMissionsPr ? mission.dateDebutMissionsPr|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateFinMissionsPr</th>
                <td>{{ mission.dateFinMissionsPr ? mission.dateFinMissionsPr|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>HoraireMissions</th>
                <td>{{ mission.horaireMissions ? mission.horaireMissions|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>TempsPause</th>
                <td>{{ mission.tempsPause }}</td>
            </tr>
            <tr>
                <th>PauseRemunere</th>
                <td>{{ mission.pauseRemunere ? 'Yes' : 'No' }}</td>
            </tr>

            <tr>
                <th>Equipement</th>
                <td>{{ mission.equipement }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ mission.etat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_missions_index') }}">back to list</a>

    <a href="{{ path('admin_missions_edit', {'idMissions': mission.idMissions}) }}">edit</a>

    {{ include('backoffice/admin_missions/_delete_form.html.twig') }}
{% endblock %}
