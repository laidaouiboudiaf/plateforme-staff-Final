    <table class="table">
        <thead>
            <tr>
                <th>TitulaireDuCompte</th>
                <th>Iban</th>
                <th>IdBancairesC</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for infos_bancaires_c in infos_bancaires_cs %}
